# RabbitMQ (Required)
AMQP_PROJECT="myproject"
AMQP_HOST="rabbitmq.internal"
AMQP_PORT="5672"
AMQP_USER="user"
AMQP_PASS="password"
AMQP_VHOST="/"
AMQP_MICROSERVICE_NAME="myservice"

# PostgreSQL (Required)
DB_BOX_HOST="postgres.internal"
DB_BOX_PORT="5432"
DB_BOX_NAME="nanoservice-myservice"
DB_BOX_USER="myservice"
DB_BOX_PASS="secret"
DB_BOX_SCHEMA="myservice"

# StatsD Metrics (Optional - disabled by default)
STATSD_ENABLED="false"
STATSD_HOST="10.192.0.15"
STATSD_PORT="8125"
STATSD_NAMESPACE="myservice"
STATSD_SAMPLE_OK="0.1"
STATSD_SAMPLE_PAYLOAD="0.1"
APP_ENV="production"

# Connection Lifecycle (Optional)
CONNECTION_MAX_JOBS="0"
